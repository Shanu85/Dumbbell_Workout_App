<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".startWorkout_activity"
    android:id="@+id/activity_start_workout_parent"
    android:padding="2dp"
    >
    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/workout_all_exercise_recyclerView"
        />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="10dp"
        android:src="@drawable/ic_baseline_play"
        android:backgroundTint="@color/orange"
        app:backgroundTint="#FFFF"
        app:rippleColor="#FFF"
        android:hapticFeedbackEnabled="true"
        android:id="@+id/selectWorkoutTimeButton"
        />
</RelativeLayout>